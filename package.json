{
    "private": true,
    "scripts": {
        "db:up": "docker-compose -f ./deploy/docker-compose.yaml up -d --build",
        "db:down": "docker-compose -f ./deploy/docker-compose.yaml down",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development parcel build resources/ts/*.ts -d public/js/ --no-minify",
        "watch": "parcel watch resources/ts/*.ts -d public/js/",
        "hot": "parcel resources/ts/*.ts -d public/js/",
        "prod": "npm run production",
        "production": "parcel build resources/ts/*.ts -d public/js/",
        "docs:setup": "node ./docs/setup.js",
        "mock-server:generate": "node ./docs/generate-api-server",
        "mock-server": "node ./docs/output/nodejs-server"
    },
    "devDependencies": {
        "@types/webpack": "^4.41.21",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "node-wget": "^0.4.3",
        "parcel-bundler": "^1.12.4",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "vue": "^3.0.0-rc.5"
    }
}
